@model Ejecuciones.Models.Fallador

@{
    ViewData["Title"] = "Juzgados Falladores";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Editar</h1>

<h3>Juzgados Falladores</h3>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="FalladorId" />
            <div class="form-group">
                <label asp-for="DepartamentoId" class="control-label"></label>
                <select asp-for="DepartamentoId" class="form-control" asp-items="ViewBag.DepartamentoId"></select>
                <span asp-validation-for="DepartamentoId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MunicipioId" class="control-label"></label>
                <select asp-for="MunicipioId" class="form-control" asp-items="ViewBag.MunicipioId"></select>
                <span asp-validation-for="MunicipioId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="JuzgadoFallador" class="control-label"></label>
                <input asp-for="JuzgadoFallador" class="form-control" />
                <span asp-validation-for="JuzgadoFallador" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Volver a la lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
