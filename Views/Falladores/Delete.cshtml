@model Ejecuciones.Models.Fallador

@{
    ViewData["Title"] = "Juzgados Falladores";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Eliminar</h1>

<h3>Juzgados Falladores</h3>

<h6>Confirma que desea eliminar el siguiente registro?</h6>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.JuzgadoFallador)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.JuzgadoFallador)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Departamento)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Departamento.NombreDepartamento)
        </dd class>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Municipio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Municipio.NombreMunicipio)
        </dd class>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="FalladorId" />
        <input type="submit" value="Eliminar" class="btn btn-primary" /> |
        <a asp-action="Index">Volver a la lista</a>
    </form>
</div>
