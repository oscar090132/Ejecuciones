@model IEnumerable<Ejecuciones.Models.Fallador>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.JuzgadoFallador)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Departamento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Municipio)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.JuzgadoFallador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Departamento.CodigoDepartamento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Municipio.CodigoMunicipio)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FalladorId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FalladorId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FalladorId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
