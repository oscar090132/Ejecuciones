        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="SolicitudId" />
        <input type="submit" value="Eliminar" class="btn btn-primary" /> |
        <a asp-action="Index">Volver a la lista</a>
    </form>
</div>
