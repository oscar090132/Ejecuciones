@model Ejecuciones.Models.Solicitud

@{
    ViewData["Title"] = "Registrar Solicitud";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Eliminar</h1>
<h3>Solicitud</h3>
<h6>Confirma que desea eliminar la siguiente solicitud?</h6>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.SolicitudId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.SolicitudId)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FechaSolicitud)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaSolicitud)
        </dd>
        <dt class="col-sm-2">
