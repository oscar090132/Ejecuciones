        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CedulaCondenado)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NombresCondenado)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NombresCondenado)
